{
  "name": "ng2-recaptcha",
  "version": "0.1.1",
  "description": "Angular 2 + TypeScript component for Google reCAPTCHA",
  "scripts": {
    "types": "typings install",
    "transpile": "./node_modules/.bin/tsc",
    "clean": "./node_modules/.bin/del-cli typings \"./recaptcha/**/*.+(js|d.ts|js.map)\" \"./ng2-recaptcha.*+(js|d.ts|js.map)\"",
    "compile": "npm run clean && npm run types && npm run transpile",
    "publish.compiled": "npm run compile && npm publish"
  },
  "main": "ng2-recaptcha.js",
  "typings": "ng2-recaptcha.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DethAriel/ng2-recaptcha"
  },
  "keywords": [
    "angular2",
    "recaptcha"
  ],
  "author": "Ruslan Arkhipau <dethariel@gmail.com>",
  "license": "MIT",
  "peerDependencies": {
    "@angular/core": "^2.0.0-rc.4",
    "@angular/common": "^2.0.0-rc.4",
    "@angular/compiler": "^2.0.0-rc.4",
    "@angular/forms": "0.2.0"
  },
  "devDependencies": {
    "@angular/core": "^2.0.0-rc.4",
    "@angular/common": "^2.0.0-rc.4",
    "@angular/compiler": "^2.0.0-rc.4",
    "@angular/forms": "^0.2.0",
    "del-cli": "0.2.0",
    "rxjs": "5.0.0-beta.6",
    "typescript": "1.8.10",
    "typings": "^1.3.2",
    "zone.js": "0.6.12"
  },
  "homepage": "https://github.com/DethAriel/ng2-recaptcha#readme"
}
