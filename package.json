{
  "name": "ng2-recaptcha",
  "version": "0.1.0",
  "description": "Angular 2 + TypeScript component for Google reCAPTCHA",
  "scripts": {
    "view-samples": "cd sample && lite-server",
    "postinstall": "typings install",
    "compile": "./node_modules/.bin/tsc",
    "clean": "./node_modules/.bin/del-cli \"./recaptcha/**/*.+(js|d.ts|js.map)\" \"./ng2-recaptcha.+(js|d.ts|js.map)\"",
    "prepublish": "npm run clean && npm run compile"
  },
  "main": "ng2-recaptcha.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DethAriel/ng2-recaptcha"
  },
  "keywords": [
    "angular2",
    "recaptcha"
  ],
  "author": "Ruslan Arkhipau <dethariel@gmail.com>",
  "license": "MIT",
  "peerDependencies": {
    "@angular/core": "^2.0.0-rc.4"
  },
  "devDependencies": {
    "@angular/core": "^2.0.0-rc.4",
    "del-cli": "0.2.0",
    "lite-server": "^2.2.0",
    "rxjs": "5.0.0-beta.6",
    "typescript": "1.8.10",
    "typings": "^1.3.2",
    "zone.js": "0.6.12"
  },
  "homepage": "https://github.com/DethAriel/ng2-recaptcha#readme"
}
